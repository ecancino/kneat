{
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "include": [".", "README.md"],
        "exclude": ["node_modules"]
    },
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true,
        "useLongnameInNav": false,
        "showInheritedInNav": true
    },
    "opts": {
        "destination": "docs",
        "encoding": "utf8",
        "private": true,
        "recurse": true
    }
}
